    "test:coverage": "vitest --ui --coverage",
    "build": "rollup -c",
    "prepare": "npm run build"
  },
  "author": "Dyllan Macias",
  "license": "MIT",
  "devDependencies": {
    "@vitest/coverage-istanbul": "^4.0.8",
