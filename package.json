  "rollup": "^3.29.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-commonjs": "^24.0.0",
    "rollup-plugin-typescript2": "^0.34.1",
  "vue": "^3.5.24",
    "typescript": "^5.2.2",
    "vitest": "^4.0.8"
  },
  "peerDependencies": {
    "vue": "^3"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs"
